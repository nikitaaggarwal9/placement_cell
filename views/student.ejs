<section id="feed-studensts">
    <h4>Posts</h4>

    <% if (locals.user) { %>
        <form action="/studensts/create" id="new-post-form" method="POST">
            <textarea name="content" cols="30" rows="3" required></textarea>
            <input type="submit" value="Post">
        </form>

        <div id="students-list-container">
            <ul>
                <% for(student of students) { %>
                    <li>
                        <%- stuent.name %>
                    </li>
                <% } %>
            </ul>
        </div>
    <% } %>

            
</section>


<section id="user-friends">
    <h4>Friends</h4>
    <% for(u of all_users) { %> 
        <p>
            <a href="/users/profile/<%= u.id %>"><%= u.name %></a>
        </p>
    <% } %>
</section>